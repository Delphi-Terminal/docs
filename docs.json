{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "name": "Delphi Terminal",
  "colors": {
    "primary": "#4F46E5",
    "light": "#818CF8",
    "dark": "#3730A3"
  },
  "styling": {
    "codeblocks": {
      "theme": "one-dark-pro"
    }
  },
  "favicon": "/DelphiWhiteLogo.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "quickstart",
              "authentication"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "openapi.json"
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "API Status",
          "href": "https://api.delphiterminal.co/health",
          "icon": "signal"
        }
      ]
    }
  },
  "logo": {
    "light": "/DelphiBlue.png",
    "dark": "/DelphiWhite.png",
    "size": 200
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@delphiterminal.co"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://delphiterminal.co"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/delphiterminal",
      "github": "https://github.com/delphiterminal"
    }
  },
  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "autogenerate": false
    }
  }
}
